{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.activeBorder": "#010101",
    "activityBar.background": "#010101",
    "activityBar.dropBorder": "#010101",
    "activityBar.foreground": "#ffffffa6",
    "activityBar.inactiveForeground": "#323f4b",
