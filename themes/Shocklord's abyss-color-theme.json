    "breadcrumb.focusForeground": "#ffffffa6",
    "breadcrumb.foreground": "#757575",
    "breadcrumbPicker.background": "#212121",
    "button.background": "#00608e",
    "button.foreground": "#ffffffa6",
    "button.hoverBackground": "#517f9c",
    "charts.blue": "#00608e",
    "charts.foreground": "#ffffffa6",
    "charts.green": "#1a9700",
    "charts.lines": "#ffffffa6",
    "charts.orange": "#d1691b",
    "charts.purple": "#4b488e",
    "charts.red": "#c13547",
    "charts.yellow": "#d2b700",
    "editor.background": "#080808",
    "editor.foreground": "#ffffffa6",
    "editor.lineHighlightBackground": "#121212",
