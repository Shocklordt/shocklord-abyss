    "editorGroup.emptyBackground": "#010101",
    "editorGroupHeader.noTabsBackground": "#010101",
    "editorGroupHeader.tabsBackground": "#010101",
    "editorGutter.background": "#080808",
    "editorLineNumber.activeForeground": "#ffffffa6",
    "editorLineNumber.foreground": "#323f4b",
    "gitDecoration.conflictingResourceForeground": "#b7b3cc",
    "gitDecoration.deletedResourceForeground": "#c8787a",
    "gitDecoration.ignoredResourceForeground": "#757575",
    "gitDecoration.modifiedResourceForeground": "#ccb664",
    "gitDecoration.stageDeletedResourceForeground": "#b7b3cc",
    "gitDecoration.stageModifiedResourceForeground": "#d59369",
    "gitDecoration.submoduleResourceForeground": "#76ab63",
    "gitDecoration.untrackedResourceForeground": "#ffffffa6",
    "menubar.selectionBackground": "#00608e",
    "panel.background": "#010101",
    "panel.border": "#010101",
    "scrollbar.shadow": "#080808",
    "sideBar.background": "#010101",
    "sideBar.foreground": "#ffffffa6",
    "sideBarSectionHeader.background": "#212121",
